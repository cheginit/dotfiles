gnome.wallpaper: ~/.dotfiles/welostthesea.jpg
i3-wm.bar.position: top
i3-wm.bar.trayoutput: primary

i3-wm.bindsym.1: c
i3-wm.bindsym.program.1: rofi -show calc -modi calc -no-show-match -no-sort
i3-wm.bindsym.2: Print
i3-wm.bindsym.program.2: maim -su | xclip -selection clipboard -t image/png
i3-wm.bindsym.3: Shift+Print
i3-wm.bindsym.program.3: maim -su ~/Pictures/$(date +%s).png
i3-wm.bindsym.4: Ctrl+Print
i3-wm.bindsym.program.4: ocr

i3xrocks.value.mediaplayerwidth: 25

i3-wm.font: Cascadia Code PL Medium 13
i3-wm.bar.font: pango:Cascadia Code PL Medium 13, Material Design Icons 13
gnome.terminal.font: Cascadia Code PL Medium 13
